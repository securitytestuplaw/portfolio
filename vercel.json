{
  "version": 2,
  "builds": [
    {
      "src": "index.html",
      "use": "@vercel/static"
    },
    {
      "src": "public/**/*",
      "use": "@vercel/static"
    },
    {
      "src": "api/**/*.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "/api/$1"
    },
    {
      "src": "/WardaKhanCV.pdf",
      "dest": "/public/WardaKhanCV.pdf"
    },
    {
      "src": "/",
      "dest": "/index.html"
    }
  ]
}
